                    "updatedAt": {
                        "type": "string"
                    },
                    "__v": {
                        "type": "integer"
                    },
                    "blast": {
                        "type": "string"
                    },
                    "bucket": {
                        "type": "string"
                    },
                    "checkIn": {
                        "type": "string"
                    },
                    "groupChat": {
                        "type": "string"
                    },
